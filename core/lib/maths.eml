func round = (var num, var places){
	return (___invoke("maths.round", num, places))
}

func floor = (var num){
	return (___invoke("maths.floor", num))
}

func ceil = (var num){
	return (___invoke("maths.ceil", num))
}

func abs = (var num){
	if(num >= 0){
		return (num)
	}else{
		return (-num)
	}
}
