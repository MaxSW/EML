obj input = import("input")
obj alert = import("alert")
obj random = import("random")

var in = input.getNumber("Choose a number between 1 and 10")

if(in > 10){
	alert.warn("Your number was too high")
} else if(in < 1){
	alert.warn("Your number was too low")
} else {
	if(in == random.between(1, 10)){
		alert.info("You won")
	}else{
		alert.info("You lost")
	}
}




 